import re

def match_pattern(input_string):
    pattern = 'a.*b'  # 'a' followed by anything, ending with 'b'

    if re.search(pattern, input_string):
        return True
    else:
        return False

# Test strings
test_string1 = 'acb'
test_string2 = 'aXb'
test_string3 = 'a followed by anything ending with b'

# Check if the string matches the pattern
print("String:", test_string1, " Matched:", match_pattern(test_string1))
print("String:", test_string2, " Matched:", match_pattern(test_string2))
print("String:", test_string3, " Matched:", match_pattern(test_string3))

